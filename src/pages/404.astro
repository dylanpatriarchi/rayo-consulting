---
import BaseLayout from "@/layouts/BaseLayout.astro";
import Error404 from "@components/infopages/Error404.astro";
import { type AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "404 Errore: Pagina Non Trovata - Rayo Consulting",
	description:
		"Ops! La pagina che stai cercando non esiste. Torna alla homepage di Rayo Consulting per i nostri servizi di sviluppo web, app mobile e soluzioni AI.",
	canonical: "https://rayo.consulting/404/",
